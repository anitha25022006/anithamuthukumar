import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner jaya= new Scanner(System.in);
        int number=jaya.nextInt();
        int n=number;
        int sum=0;
        while(number>0)
        {
            sum+=number%10;
            number/=10;
         }
        if(n%sum==0)
        {
            System.out.println("Harshad Number");
        }
        else {
            System.out.println("Not Harshad Number");
           }
     }
}
