import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int i=sc.nextInt();
        int j=sc.nextInt();
        int sum=0;
         if(i>0 && j>0 && i<30 && j<30 && i<j){
        for(int a=i;a<=j;a++){
           if(a%2==0)
           {
            sum+=a;
           } 
        }
             System.out.println(sum);
      }
     else
       System.out.println("Invalid Input"); 
    }
}
